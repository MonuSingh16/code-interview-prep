     "text": [
      "{'alice': 1, 'brad': 2, 'collin': 1, 'dylan': 1, 'kim': 1}\n"
     ]
    }
   ],
   "source": [
    "names = [\"alice\", \"brad\", \"collin\", \"brad\", \"dylan\", \"kim\"]\n",
    "\n",
    "countMap = {}\n",
    "for name in names:\n",
    "    if name not in countMap:\n",
    "        countMap[name] = 1\n",
    "    else:\n",
    "        countMap[name] += 1\n",
    "print(countMap)"
